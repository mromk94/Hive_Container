<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#FF101016" />
            <stroke
                android:width="2dp"
                android:color="#FFD4AF37" />
        </shape>
    </item>
    <item
        android:left="8dp"
        android:top="8dp"
        android:right="8dp"
        android:bottom="8dp">
        <shape android:shape="oval">
            <gradient
                android:type="radial"
                android:gradientRadius="24dp"
                android:startColor="#FFFFE7A0"
                android:endColor="#FFD4AF37" />
        </shape>
    </item>
</layer-list>
